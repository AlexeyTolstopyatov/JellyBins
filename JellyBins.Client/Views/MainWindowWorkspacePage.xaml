<Page x:Class="JellyBins.Client.Views.MainWindowWorkspacePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:JellyBins.Client.ViewModels"
      mc:Ignorable="d"
      Title="InfoPage"
      DataContext="{x:Static local:MainWindowViewModel.Instance}"
      Height="Auto" Width="Auto">
    <Grid>
        <Frame
            Margin="20"
            Content="{Binding PageContainer, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</Page>
