<Application x:Class="jellybins.Fluent.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:jellybins.Fluent"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             xmlns:mica="clr-namespace:MicaWPF.Styles;assembly=MicaWPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 <ui:ControlsDictionary/>
                 <ui:ThemesDictionary Theme="Light"/>
                 <mica:ControlsDictionary/>
                 <mica:ThemeDictionary Theme="Auto" DesignModeTheme="Light"/>
             </ResourceDictionary.MergedDictionaries>
         </ResourceDictionary>
    </Application.Resources>
</Application>
